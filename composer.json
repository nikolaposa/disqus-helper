{
    "name": "nikolaposa/disqus-helper",
    "description": "PHP library which facilitates integration of Disqus widgets.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "disqus",
        "widget",
        "helper"
    ],
    "homepage": "https://github.com/nikolaposa/disqus-helper",
    "authors": [
        {
            "name": "Nikola Po≈°a",
            "email": "posa.nikola@gmail.com",
            "homepage": "http://www.nikolaposa.in.rs"
        }
    ],
    "require": {
        "php": "~7.0"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "^1.11",
        "phpunit/phpunit": "^5.0"
    },
    "autoload": {
        "psr-4": { "DisqusHelper\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "DisqusHelper\\Tests\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
